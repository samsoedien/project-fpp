{
  "name": "food-printing-platform",
  "version": "1.0.0",
  "description": "An online platform for creating, sharing and modifying digital CAD files for 3D food printing.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/samsoedien/project-fpp.git"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "set PORT=4000 && node server.js",
    "server": "set PORT=4000 && nodemon server.js --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "test": ""
  },
  "keywords": [
    "food",
    "3D",
    "printing",
    "chef",
    "cooking",
    "preparation",
    "consumption",
    "restaurant",
    "digital",
    "CAD",
    "data",
    "experience"
  ],
  "author": "Samsoedien <n.samsoedien@student.tue.nl> (https://samsoedien.com)",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^3.6.1",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.2",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "eslint": "5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  }
}
